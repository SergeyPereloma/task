import sys
import time

def main():
    print("Starting the count...")

    for i in range(1, 51):
        if i % 2 == 1:  # Check if the number is odd
            print(f"Count: {i}", file=sys.stderr)
        else:
            print(f"Count: {i}")
        time.sleep(0.1)

if __name__ == "__main__":
    main()
